{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Run Extension",
			"program": "${workspaceFolder}/node_modules/vscode/bin/vscode",
			"args": [
				"${workspaceFolder}/out/src/extension"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			],
			"preLaunchTask": "npm: compile"
		}
	]
}